{
    "rules": {
        "no-set-state": "off",
        "no-console": "off",
        "no-unused-vars": "off",
        "no-case-declarations": "off"
    },
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "extends": [
      "eslint:recommended"
    ]
}
